{
  "name": "generator-rr-boilerplate",
  "version": "0.0.1",
  "description": "The boilerplate for generator-rr",
  "main": "",
  "scripts": {
    "build": "cross-env NODE_ENV=production npm run clean && webpack --verbose --colors --display-error-details --config webpack.config.prod.js",
    "clean": "rimraf static/bundle.js static/bundle.js.map",
    "start": "parallelshell \"node ./server.js\" \"npm run test:watch\"",
    "start:prod": "cross-env NODE_ENV=production NODE_PATH=./src node ./server.js",
    "lint": "eslint src",
    "test": "babel-tape-runner test/**/*.spec.js | faucet",
    "test:watch": "watch \"npm test\" test/",
    "check": "npm run lint && npm run test",
    "postinstall": "npm run build"
  },
  "author": {
    "name": "Tucker Connelly",
    "email": "web@tuckerconnelly.com",
    "url": "http://tuckerconnelly.com/"
  },
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/KaleoSoftware/generator-rr-boilerplate.git"
  },
  "dependencies": {
    "clean-webpack-plugin": "^0.1.6",
    "compression": "^1.6.1",
    "cross-env": "^1.0.7",
    "debounce": "^1.0.0",
    "express": "^4.13.3",
    "file-loader": "^0.8.5",
    "history": "^2.0.0-rc2",
    "immutable": "^3.7.6",
    "isomorphic-fetch": "^2.2.0",
    "material-ui": "^0.14.1",
    "path": "^0.12.7",
    "piping": "^0.3.0",
    "radium": "^0.16.2",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-intl": "^2.0.0-beta-2",
    "react-redux": "^4.0.6",
    "react-router": "^2.0.0-rc5",
    "react-router-redux": "^2.1.0",
    "react-style": "^0.5.5",
    "react-tap-event-plugin": "^0.2.1",
    "redux": "^3.0.5",
    "redux-devtools": "^3.0.1",
    "redux-fetch-dispatch": "0.0.6",
    "redux-thunk": "^1.0.3",
    "rimraf": "^2.5.0",
